{
  "name": "typescript-lib-template",
  "version": "0.1.0",
  "description": "TypeScript Lib template.",
  "main": "src/index.ts",
  "author": "laamginghong",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
